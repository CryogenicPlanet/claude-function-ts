{
  "name": "claude-function-ts",
  "version": "0.1.3-bun-esm",
  "type": "module",
  "devDependencies": {
    "@cryop/zpp": "^0.0.3",
    "@types/bun": "latest",
    "@types/json-schema": "^7.0.15",
    "esbuild": "^0.20.2",
    "openai": "^4.29.2",
    "tsup": "^8.0.2",
    "typescript": "^5.4.3",
    "zod": "^3.22.4"
  },
  "peerDependencies": {
    "typescript": "^5.0.0",
    "@anthropic-ai/sdk": "^0.19.0",
    "openai": "^4.29.2"
  },
  "scripts": {
    "tsc": "tsup src/index.ts --format esm --external @anthropic-ai/sdk --dts-only",
    "build": "bun build src/index.ts --outdir ./dist --target node --format esm --external @anthropic-ai/sdk --splitting --sourcemap=external && bun run tsc",
    "esbuild": "esbuild src/index.ts --format=esm --outdir=./dist --external:@anthropic-ai/sdk --bundle --platform=node",
    "prepare": "bun run build"
  },
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js"
    },
    "./openai": {
      "import": "./dist/openai.js"
    }
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@anthropic-ai/sdk": "^0.19.0",
    "dedent": "^1.5.1",
    "fast-xml-parser": "^4.3.6"
  }
}